PROGRAM GLOBAL_DECLARE PGM TYPE int add ( F_ARG TYPE int a  , TYPE int b  ) { STMT STMT_RETURN return EXP EXP a + EXP b ;  } PGM TYPE void show ( F_ARG TYPE char a  ) { STMT PRINTFUNC print ( EXP a ) ;  } PGM TYPE int main ( F_ARG ) { STMT STMT_DECLARE TYPE int a = EXP 3 ; STMT STMT_DECLARE TYPE int b = EXP EXP a + EXP 1 ; STMT CALL add ( ARGS EXP EXP a + EXP b ) ; STMT STMT_DECLARE TYPE char compiler = EXP 'c' ; STMT CALL show ( ARGS ) ; STMT STMT_RETURN return EXP 0 ;  } 